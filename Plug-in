#include <bits/stdc++.h>
using namespace std;

void fn(string &s) {
    for (int i = 0; i < (int)s.size() - 1; i++) {
        if (s[i] == s[i + 1]) {
            s.erase(i, 2);
            fn(s);      
            return;     
        }
    }
}

int main() {
    string s;
    cin >> s;
    fn(s);
    cout << s;
    return 0;
}
